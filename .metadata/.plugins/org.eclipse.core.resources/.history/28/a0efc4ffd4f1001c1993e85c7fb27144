package test;

import java.util.Scanner;


public class Main {
	public static void main(String[] args) throws Exception {
		DB_Task dt = new DB_Task();
		Scanner sc = new Scanner(System.in);
		
		System.out.print("이름 : ");
		String name = sc.next();
		System.out.print("주소 : ");
		String juso = sc.next();
		System.out.print("핸드폰번호 : ");
		String phone = sc.next();
		
		dt.insert(name, juso, phone);
	}
}
