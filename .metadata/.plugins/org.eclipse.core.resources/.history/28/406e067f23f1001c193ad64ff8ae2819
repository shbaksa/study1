	<script>
		function check()
		{
			alert(event.keyCode);
		}
	</script>
	
</head>
<body>	<!-- keyevent3.jsp : keydown, keypress의 차이 -->
	
	<input type="text" onkeydown="check()"> <p>
	<input type="text" onkeypress="check())"> <p>
	
	
	
