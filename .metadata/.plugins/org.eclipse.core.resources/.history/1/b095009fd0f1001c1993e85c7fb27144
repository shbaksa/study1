package tset;

import java.util.Scanner;
import java.util.Random;

public class Random {
	public static void main(String[] args) {
		Random rand = new Random();
		Scanner sc = new Scanner(System.in);
		
		System.out.print("a : " );
		int a = sc.nextInt();
		System.out.print("b : ");
		int b = sc.nextInt();
		
		int num = rand.nextInt(a,b+1);
		
	}

	
